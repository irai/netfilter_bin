[Unit]
Description=netfilter download updates
Wants=network-online.target
After=network.target network-online.target

[Service]
WorkingDirectory=/home/netfilter/spinifex/bin/netfilter_bin
Type=oneshot
User=root

ExecStart=/home/netfilter/spinifex/bin/download.script start
ExecStop=/home/netfilter/spinifex/bin/download.script stop

#TimeoutSec=240

[Install]
WantedBy=multi-user.target
